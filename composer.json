{
    "name": "mugen/laravel-workerman-gateway",
    "description": "Laravel 5 Workerman Gateway worker",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "aki",
            "email": "mj501738931@126.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "^5.5",
        "workerman/gateway-worker": "^3.0",
        "workerman/gatewayclient": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mugen\\LaravelWorkermanGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mugen\\LaravelWorkermanGateway\\GatewayServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
